if (params.chipatlas_tissue_types) {
    process {
        withName: 'SAMTOOLS_SORT' {
            ext.prefix = { "${meta.id}.sorted" }
        }
    }
    process {
        withName: 'CONCATENATE_EH_ATLAS' {
            ext.prefix = { "eh" }
        }
    }
    process {
        withName: 'CONCATENATE_CHIP_ATLAS' {
            ext.prefix = { "chipatlas" }
        }
    }
}